<placeName corresp="#place_築地">
        築地
       </placeName>
       新喜楽ニ抵リ
       <orgName>
        第一銀行
       </orgName>
       ニ於テ
       <persName corresp="#pers_伊藤">
<surname>
         伊藤
        </surname>
</persName>
       統監、其他
       <persName corresp="#pers_鶴原">
<surname>
         鶴原
        </surname>
</persName>
       、
       <persName corresp="#pers_荒井">
<surname>
         荒井
        </surname>
</persName>
       、
       <persName corresp="#pers_木内">
