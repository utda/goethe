       <orgName>
        十勝開墾会社
       </orgName>
       重役会ヲ開キ
       <persName corresp="#pers_大倉">
<surname>
         大倉
        </surname>
</persName>
       氏、同姓
       <persName corresp="#pers_渋沢成一郎">
<forename>
         喜作
        </forename>
</persName>
       其他
       <persName corresp="#pers_渡辺">
<surname>
         渡辺
        </surname>
</persName>
       、
       <persName corresp="#pers_町村">
<surname>
         町村
        </surname>
</persName>
       、
       <persName corresp="#pers_小田">
<surname>
         小田
        </surname>
</persName>
       ノ数氏来ル、瓦斯会社
       <persName corresp="#pers_福島">
<surname>
         福島
        </surname>
</persName>
       氏、銀行書記
       <persName corresp="#pers_清水">
<surname>
         清水
        </surname>
</persName>
       氏来ル
